{
  "name": "@qlik-oss/react-native-simple-grid",
  "version": "0.3.9",
  "description": "A straight table component designed for performance",
  "main": "src/index",
  "scripts": {
    "test": "jest",
    "ts-check": "npx tsc",
    "lint": "yarn eslint && yarn swiftlint && yarn javalint",
    "eslint": "npx eslint ./ --ignore-pattern node_modules --fix --quiet",
    "swiftlint": "swiftlint",
    "javalint": "cd android && ./gradlew lint",
    "ci:install": "yarn install --frozen-lockfile"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "files": [
    "android/",
    "ios/",
    "src/",
    "react-native-simple-grid.podspec",
    "babel.config.js",
    "jest.config.js",
    "jest.setup.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/react-native-simple-grid"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/qlik-oss"
  },
  "author": "Qlik <vel@qlik.com> (https://github.com/qlik-oss/react-native-simple-grid)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qlik-oss/react-native-simple-grid/issues"
  },
  "homepage": "https://github.com/qlik-oss/react-native-simple-grid#readme",
  "devDependencies": {
    "@react-native-community/eslint-config": "^2.0.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/jest": "27.4.0",
    "@types/react": "16.9.19",
    "@types/react-native": "0.66.15",
    "eslint": "7.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.1.3",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "semantic-release": "19.0.2",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  }
}

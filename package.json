{
  "name": "@qlik/react-native-simple-grid",
  "version": "1.2.12",
  "description": "A react native straight table component designed for performance",
  "main": "src/index",
  "scripts": {
    "test": "jest",
    "ts-check": "npx tsc",
    "lint": "yarn eslint && yarn swiftlint && yarn javalint",
    "eslint": "npx eslint ./ --max-warnings=0",
    "swiftlint": "cd ios && swiftlint --strict",
    "javalint": "cd android && ./gradlew lint -x compileDebugJavaWithJavac",
    "ci:install": "yarn install --frozen-lockfile",
    "pod-install": "cd example/TableApp/ios && pod install",
    "setup-dev": "yarn && cd example/TableApp && yarn"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "files": [
    "android/",
    "ios/",
    "src/",
    "react-native-simple-grid.podspec",
    "babel.config.js",
    "jest.config.js",
    "jest.setup.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/react-native-simple-grid"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "Qlik <vel@qlik.com> (https://github.com/qlik-oss/react-native-simple-grid)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qlik-oss/react-native-simple-grid/issues"
  },
  "homepage": "https://github.com/qlik-oss/react-native-simple-grid#readme",
  "devDependencies": {
    "@qlik/react-native-carbon": "^2.1.16",
    "@react-native-community/eslint-config": "^3.0.1",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/jest": "27.4.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "18.0.28",
    "@types/react-native": "0.66.15",
    "@types/react-native-vector-icons": "^6.4.13",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "27.5.1",
    "jotai": "1.13.1",
    "prettier": "2.8.5",
    "react": "18.2.0",
    "react-native": "0.64.1",
    "react-native-gesture-handler": "2.9.0",
    "react-native-paper": "4.12.0",
    "react-native-svg": "^13.8.0",
    "semantic-release": "19.0.2",
    "typescript": "<4.10"
  },
  "peerDependencies": {
    "jotai": "~1",
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": "2.9.0",
    "react-native-paper": "4.12.0",
    "react-native-vector-icons": "9.0.0"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  }
}
